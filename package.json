{
  "name": "algo-repo",
  "version": "1.0.0",
  "description": "A website hosting a repository of algorithms which users can use and publish",
  "author": "Daniel Carl Jones <dcjones1@sheffield.ac.uk>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "db:seed": "babel-node scripts/seed.js",
    "db:drop": "babel-node scripts/drop.js",
    "db:reset": "yarn run db:drop && yarn run db:seed",
    "test": "ava"
  },
  "dependencies": {
    "@nuxtjs/vuetify": "^0.4.1",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "cross-env": "^5.0.1",
    "express": "^4.15.3",
    "express-basic-auth": "^1.1.5",
    "express-session": "^1.15.6",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "mongoose": "^5.0.12",
    "morgan": "^1.9.0",
    "nuxt": "^1.0.0-rc3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.0",
    "pegjs": "^0.10.0",
    "source-map-support": "^0.4.15",
    "vue-particles": "^1.0.9"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "ava": "^1.0.0-beta.3",
    "babel-eslint": "^7.2.3",
    "backpack-core": "^0.4.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "jsdom": "^11.7.0"
  },
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  }
}
